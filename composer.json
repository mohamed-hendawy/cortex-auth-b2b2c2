{
    "name": "cortex/auth-b2b2c2",
    "type": "cortex-module",
    "description": "Cortex Auth B2B2C2 (Business to Business, Business to Customer, Business to Business to Customer) is a complementary module for cortex/auth which is frontend layer for the powerful authentication, authorization and verification package rinvex/auth on top of Laravel.",
    "keywords": [
        "authentication",
        "authorization",
        "verification",
        "activation",
        "abilities",
        "cortex",
        "phone",
        "sms",
        "call",
        "users",
        "module",
        "security",
        "twofactor",
        "laravel",
        "rinvex",
        "rbac",
        "totp",
        "acl",
        "roles"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-auth/issues",
        "source": "https://github.com/rinvex/cortex-auth",
        "docs": "https://github.com/rinvex/cortex-auth/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-auth/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "Illuminate/auth": "~5.6.0",
        "Illuminate/console": "~5.6.0",
        "Illuminate/http": "~5.6.0",
        "Illuminate/routing": "~5.6.0",
        "Illuminate/support": "~5.6.0",
        "cortex/foundation": "dev-develop",
        "davejamesmiller/laravel-breadcrumbs": "^5.0.0",
        "laravel/socialite": "^3.0.0",
        "laravelcollective/html": "~5.6.0",
        "league/fractal": "^0.17.0",
        "nesbot/carbon": "^1.22.0",
        "pragmarx/google2fa": "^2.0.0",
        "proengsoft/laravel-jsvalidation": "^2.2.0",
        "propaganistas/laravel-phone": "^4.0.0",
        "rinvex/auth": "dev-develop",
        "rinvex/composer": "dev-develop",
        "rinvex/country": "dev-develop",
        "rinvex/menus": "dev-develop",
        "rinvex/support": "dev-develop",
        "rinvex/tenants": "dev-develop",
        "silber/bouncer": "v1.0.0-rc.1",
        "spatie/laravel-activitylog": "^2.5.0",
        "yajra/laravel-datatables-buttons": "^3.2.0",
        "yajra/laravel-datatables-fractal": "^1.1.0",
        "yajra/laravel-datatables-html": "^3.6.0",
        "yajra/laravel-datatables-oracle": "^8.4.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.6.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Auth\\B2B2C2\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Auth\\B2B2C2\\Providers\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
